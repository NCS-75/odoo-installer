<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Relationships -->
    <record id="authorized" model="res.partner.relation">
        <field name="code">1</field>
        <field name="name">Authorized</field>
        <field name="sequence">1</field>
    </record>
    <record id="accredited" model="res.partner.relation">
        <field name="code">2</field>
        <field name="name">Accredited</field>
        <field name="sequence">2</field>
    </record>
    <record id="certified" model="res.partner.relation">
        <field name="code">3</field>
        <field name="name">Certified</field>
        <field name="sequence">3</field>
    </record>
    <record id="premium" model="res.partner.relation">
        <field name="code">4</field>
        <field name="name">Premium</field>
        <field name="sequence">4</field>
        <field name="role_ids" eval="[(6, 0, [ref('partner_role.role_installer')])]"/>
    </record>
    <record id="unauthorized" model="res.partner.relation">
        <field name="code">0</field>
        <field name="name">Unauthorized</field>
        <field name="sequence">99</field>
    </record>
</odoo>
